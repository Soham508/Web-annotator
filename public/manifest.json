{
    "manifest_version": 3,
    "name": "web annotator",
    "version": "1.0.1",
    "icons": {
        "32": "logo.png"
    },
    "background": {
      
    },
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
       "activeTab",
    "scripting",
    "tabs",
    "storage"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "persistent": true
}